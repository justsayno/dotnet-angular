{
  "name": "dotnet-angular",
  "version": "0.0.1",
  "description": "A click through for Dynamics 365 Customer Insights",
  "scripts": {
    "build": "rimraf server/wwwroot && webpack --bail --progress --profile",
    "clean": "node ./scripts/clean",
    "dotnet-publish": "npm run clean && node ./scripts/dotnet-publish.js",
    "lint": "eslint src",
    "server": "webpack-dev-server --history-api-fallback --inline --progress --port 3000",
    "start": "npm run server",
    "start:dist": "cd server && dotnet run",
    "test": "karma start",
    "test-watch": "karma start --auto-watch --no-single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/justsayno/dotnet-angular"
  },
  "author": "Seth Reid",
  "license": "MIT",
  "dependencies": {
    "angular": "^1.5.0",
    "angular-mocks": "^1.5.0",
    "angular-ui-router": "1.0.0-beta.1",
    "angular-un-svg": "^1.0.4",
    "babel-polyfill": "^6.16.0",
    "jquery": "^3.1.1",
    "materialize-css": "^0.97.8"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "azure-cli": "^0.10.7",
    "azure-scripty": "^1.0.1",
    "azure-storage": "^1.3.2",
    "babel-core": "^6.2.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-0": "^6.16.0",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.0",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-config-angular": "^0.5.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "fs": "0.0.2",
    "fs-promise": "^1.0.0",
    "html-webpack-plugin": "^2.24.1",
    "isparta-loader": "^2.0.0",
    "jasmine-core": "^2.3.4",
    "karma": "^1.1.0",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "ncp": "^2.0.0",
    "ng-annotate-loader": "^0.2.0",
    "nginject-loader": "^2.1.0",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^3.13.0",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.13",
    "postcss-loader": "^1.1.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "rimraf-promise": "^2.0.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.0",
